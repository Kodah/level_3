#include <iostream>

using namespace std;

int main()
{
	int sum = 0,num = 0;

	cout << "Enter numbers below of 50 to sum: " << endl;

	for (int i = 0; i < 5; i++)
	{
		cin >> num;

		if (num > 50)
		{
			cout << " the number is bigger than 50 & won't be calculated.\n " << endl;
			continue;
		}

		sum += num;
	}

	cout << "\nThe sum of the numbers is: " << sum << endl;

	return 0;
}
