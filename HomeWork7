#include <iostream>

using namespace std;

int main()
{
	
	int a,b;
	cout<<"Enter the value of A: ";
	cin>>a;
	cout<<endl;
	cout<<"Enter the value of B: ";
	cin>>b;
	cout<<endl;
	
	cout<< a<<" = "<< b<<" is "<< (a==b) <<endl;
	cout<< a<<" != "<< b<<" is "<< (a!=b) <<endl;
	cout<< a<<" > "<< b<<" is "<< (a>b) <<endl;
	cout<< a<<" < "<< b<<" is "<< (a<b) <<endl;
	cout<< a<<" >= "<< b<<" is "<< (a>=b) <<endl;
	cout<< a<<" <= "<< b<<" is "<< (a<=b) <<endl;

    return 0;
}
